# The Root Query for the application
type Query {
    Customer(id: ID): Customer
    Address(id: ID): Address
}

type Customer {
    id: ID
    firstName: String
    lastName: String
    registroGeral: Int
    field1: Int
    field2: String
    address: Address
    bank: Ban
}

type Address {
    id: ID
    address: String
    number: Int
    CEP: Int
    city: String
    state: String
}

type Bank {
    id: ID
    nameBank: String
    account: String
    type: String
}

